generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url = env("DATABASE_URL")
}

model Student {
  id        Int      @id @default(autoincrement())
  fullName      String
  email     String   @unique
  telphone  Int      @unique
  NIF       String   @unique
  school    String
  Year      String
  Turno     String
  AreaInterest String
  skills   String?
  password  String
  createdAt DateTime @default(now())
}
